       <li>
        <%= link_to '投稿フォーム', new_quest_path %>
      </li>


<% @quests.each do |quest| %>
<div>
  <p>タイトル：<%= quest.title %></p>
  <p>内容：<%= quest.content %></p>
  <p>
   <% if quest.is_solved == false %>
    未解決
   <% else %>
    解決済 
   <% end %>
  </p>
  <p>ユーザーネーム：<%= quest.user.name %></p>
</div>
<% end %>